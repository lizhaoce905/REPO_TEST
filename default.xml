<?xml version="1.0" encoding="UTF-8" ?>
<manifest>
    <!-- 
    remote: 远程代码库配置
    name: 远程仓库名，在使用 git clone 时默认为 origin
    fetch: 远程仓库地址，用户和 project 节点中的 name 一起确定项目地址
    -->
    <!-- remote server -->
    <remote
        name="github"
        fetch="https://github.com" />
  <!--
    default: 默认配置
    remote: 默认使用的远程仓库
    revision: 默认使用的分支
    sync-j: 同步代码时的并发数
    -->
    <default
        remote="github"
        revision="master"
        sync-j="1" />
</manifest>
